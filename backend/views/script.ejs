<script>

  const deleteElement = async (name, el) => {
    fetch(`/entity/delete/${name}/${el}`, {
      method: "DELETE",
      headers: {
      'Content-Type': 'application/json'
      }
    }).then(res => console.log(res.json()));
  }

  const addElement = async (name) => {
    let value = prompt("Enter value");
    
    fetch(`/entity/add/${name}`, {
      method: "POST",
      headers: {
        'Content-Type': 'application/json'
      },
      body: JSON.stringify({value: "Rebecca Kuang"})
    }).then(res => console.log(res.json()));
  }

</script>